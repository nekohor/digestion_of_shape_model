{
  "gitbook": "3.x.x",
  "title": "宇阳的板形模型私房菜",
  "author": "王宇阳",
  "styles": {
    "website": "styles/website.css",
    "ebook": "styles/ebook.css",
    "pdf": "styles/pdf.css",
    "mobi": "styles/mobi.css",
    "epub": "styles/epub.css"
  },
  "plugins": [
    "prism",
    "prism-themes",
    "katex",
    "-search",
    "-hightlight"
  ],
  "pluginsConfig": {
    "prism": {
      "css": [
        "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
      ],
      "lang": {
        "flow": "cpp"
      }
    },
    "autocover": {
        "font": {
            "size": null,
            "family": "方正清刻本悦宋简体",
