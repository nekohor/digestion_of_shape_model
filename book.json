        "size": {
            "w": 1800,
            "h": 2360
        },
        "background": {
            "color": "#09F"
        }
    },
    "theme-default": {
      "showLevel": true
    }
  },
  "links": {
    "gitbook": false,
    "sharing": {
      "google": false,
      "facebook": false,
      "twitter": false,
      "all": false
    }
  },
  "pdf": {
    "pageNumbers": true,
    "paperSize": "b5",
    "fontSize": 17,
    "fontFamily":"方正清刻本悦宋简体",
